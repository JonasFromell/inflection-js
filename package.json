{
  "name": "inflection-js",
  "version": "0.0.1",
  "description": "Lightweight inflection library for JavaScript",
  "main": "lib/index.js",
  "scripts": {
    "prep": "npm run test && npm run build",
    "test": "mocha $npm_package_options_mocha",
    "build": "rm -rf lib/* && babel src --ignore __tests__ --out-dir lib"
  },
  "options": {
    "mocha": "--require scripts/mocha-bootload src/**/__tests__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonasFromell/inflection-js.git"
  },
  "keywords": [
    "inflection",
    "javascript",
    "pluralize",
    "singularize"
  ],
  "author": "Jonas Fromell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonasFromell/inflection-js/issues"
  },
  "homepage": "https://github.com/JonasFromell/inflection-js#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4"
  }
}
